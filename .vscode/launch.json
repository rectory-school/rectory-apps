    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver"
            ],
            "django": true
        },
        {
            "name": "Run jobs",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "run_jobs"
            ]
        },
        {
            "name": "SIS sync",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "sis_sync"
            ]
        },
        {
            "name": "Show permissions",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "show_permissions"
            ]
        }
    ]
}